public static void Pattern17()
	{
		int k=0;
		for(int i=1;i<=9;i++)
		{
			k+=i<=5?1:-1;
			for(int j=1;j<=9;j++)
			{
				if(j<=(6-k)||j>=(4+k))
				{
					System.out.print("*");
					
				}else
				{
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}
